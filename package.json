  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "tsc:test": "tsc -p tsconfig.test.json -w",
    "scss:tsm": "tsm \"src/**/*.scss\" --nameFormat camel --includePaths src/styles --aliasPrefixes.~styles src/styles",
    "scss": "stylelint \"src/**/*.scss\" --cache --cache-location .cache/.stylelintcache",
    "fix:scss": "stylelint \"src/**/*.scss\" --fix",
    "eslint": "eslint -c .eslintrc --ext .js,.jsx,.ts,.tsx 'src/**/*.{ts,tsx}'",
    "fix:eslint": "yarn eslint --fix",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production webpack -p --config  ./script/webpack/index.ts --env.mode production",
    "start": "webpack-dev-server --hot --watch --progress --config ./script/webpack/index.ts --env.mode development",
    "fix:all": "yarn fix:scss && yarn fix:eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/*.{js,jsx,ts,tsx,json,css}": [
      "prettier --write",
      "yarn fix:scss",
      "yarn fix:eslint"
    ]
  }
}
